{
  "manifest_version": 3,
  "name": "LLMBUS",
  "version": "1.2",
  "description": "AI red-teaming tool",
  "permissions": ["tabs", "storage"],
  "action": {
    "default_title": "LLMBUS"
  },
  "background": {
    "service_worker": "background.js"
  },
  "web_accessible_resources": [{
    "resources": ["redoperator.html"],
    "matches": ["<all_urls>"]
  }]
}
